<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>login.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savegrants</servlet-name>
        <servlet-class>grants.savegrants</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dataPulls</servlet-name>
        <servlet-class>Ajax.dataPulls</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuditTrail</servlet-name>
        <servlet-class>Ajax.AuditTrail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savesolicitaion</servlet-name>
        <servlet-class>solicitation.savesolicitaion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>copytemplates</servlet-name>
        <servlet-class>General.copytemplates</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>downloadFiles</servlet-name>
        <servlet-class>General.downloadFiles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveapplicants</servlet-name>
        <servlet-class>applicants.saveapplicants</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savegrants</servlet-name>
        <url-pattern>/savegrants</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dataPulls</servlet-name>
        <url-pattern>/dataPulls</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuditTrail</servlet-name>
        <url-pattern>/AuditTrail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savesolicitaion</servlet-name>
        <url-pattern>/savesolicitaion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>copytemplates</servlet-name>
        <url-pattern>/copytemplates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>downloadFiles</servlet-name>
        <url-pattern>/downloadFiles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveapplicants</servlet-name>
        <url-pattern>/saveapplicants</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
